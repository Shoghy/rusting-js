    "@swc/core": "^1.7.26",
    "@types/bun": "latest",
    "eslint": "9.x",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-file-extension-in-import-ts": "^2.1.1",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "3.3.3",
    "tslib": "^2.6.3",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.13.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "node": {
        "types": "./index.d.cts",
        "default": "./index.cjs"
      },
      "default": {
        "types": "./index.d.ts",
        "default": "./index.js"
      }
    },
