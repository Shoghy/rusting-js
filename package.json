  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "build": "tsup && bun run move_files.ts",
    "test": "bun test",
    "eslint": "eslint . --ignore-pattern \"dist/\"",
    "eslint:fix": "eslint . --ignore-pattern \"dist/\" --fix"
  },
  "devDependencies": {
    "@anatine/esbuild-decorators": "^0.2.19",
